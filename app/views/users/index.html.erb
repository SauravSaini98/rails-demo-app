<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>



<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Contact</th>
    <th colspan="2">Action</th>
  </tr>
  </thead>
<% @users.each do |user| %>
  <tbody>
  <tr>
    <td><%= user.first_name %></td>
    <td><%= user.email %></td>
    <td><%= user.contact %></td>
    <td><%= user.contact %></td>
    <td><%= link_to("Show", user_path(user)) %></td>
    <td><%= link_to("Edit", edit_user_path(user)) %></td>
  </tr>

  </tbody>
<% end %>

</table>